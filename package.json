{
  "name": "@jecfe/react-design-system",
  "version": "1.1.0",
  "description": "React design system",
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src/tailwind.css"
  ],
  "scripts": {
    "build": "rm -rf /dist && npm run build:esm",
    "build:esm": "tsc && cp -r tailwind.config.js ./dist/esm/tailwind.config.js && tailwindcss build -i src/tailwind.css -o dist/esm/tailwind.css",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs && cp -r ./src/css ./dist/cjs/css && cp -r tailwind.config.js ./dist/cjs/tailwind.config.js && tailwindcss build -i src/tailwind.css -o dist/cjs/tailwind.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs-build",
    "convert-svgs": "npx @svgr/cli --out-dir src/components/react-svg --ignore-existing --no-prettier --typescript src/components/icons && prettier --write ./src/components/react-svg"
  },
  "author": "Jessica Fealy",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/addon-links": "7.6.6",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/addon-styling-webpack": "0.0.5",
    "@storybook/blocks": "7.6.6",
    "@storybook/react": "7.6.6",
    "@storybook/react-vite": "7.6.6",
    "@storybook/react-webpack5": "7.6.6",
    "@storybook/test": "7.6.6",
    "@svgr/cli": "8.1.0",
    "@svgr/webpack": "8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-vertical-timeline-component": "^3.3.6",
    "autoprefixer": "10.4.16",
    "copyfiles": "^2.4.1",
    "css-loader": "6.8.1",
    "postcss": "8.4.32",
    "postcss-loader": "7.3.3",
    "prettier": "3.1.1",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.27.2",
    "storybook": "7.6.6",
    "style-loader": "3.3.3",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "publishConfig": {
    "JeCFe:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "3.5.0",
    "@codaworks/react-glow": "^1.0.6",
    "class-variance-authority": "0.7.0",
    "classnames": "^2.5.1",
    "react-hook-form": "^7.53.2",
    "react-intersection-observer": "^9.8.1"
  }
}
